function displayMeasures(content) {
  const target = document.getElementById('user_measure_form');
  target.innerHTML = content;
};
function displayFlash(content) {
  const flash = document.getElementById('flash');
  flash.innerHTML = content;
};

displayFlash('<%= j render partial: "shared/flashes" %>');
displayMeasures('<%= j render "form", user: @user %>');

